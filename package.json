{
  "name": "ai-support-agent",
  "version": "1.0.0",
  "description": "Fully autonomous AI-powered customer support agent using Node.js, LangChain, and LangGraph",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && tsc && npm run build:ui",
    "build:ui": "vite build",
    "start": "node dist/index.js",
    "dev": "concurrently \"tsx watch src/index.ts\" \"vite --port 5173\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "keywords": [
    "ai",
    "customer-support",
    "langchain",
    "langgraph",
    "llm",
    "autonomous-agent"
  ],
  "author": "",
  "license": "ISC",
